# .env.example

# Database Configuration (Used by docker-compose for production/deployment)
DB_USER=postgres
DB_PASSWORD=admin
DB_NAME=smart_inventory
DB_HOST=db        # 'db' refers to the database service name in docker-compose.yml
DB_PORT=5432

# Backend Server Configuration (Used by docker-compose)
PORT=3000

# API Key from Replicate.com (Required for AI features)
REPLICATE_API_KEY=r8_...PASTE_YOUR_REPLICATE_KEY_HERE